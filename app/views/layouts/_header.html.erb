 <header class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <%= link_to image_tag("logo.png", alt: "Edacio"), 'http://www.edacio.com', id: "logoimg" %>
          <%= link_to "Edacio | Relationships Redefined", '#', id: "logo" %>
           <script type="IN/Login" data-onAuth="onLinkedInLoad"></script>
          <nav>
            <ul class="nav pull-right">
              <li><%= link_to "Home",    root_path %></li>
				  <li><%= link_to "About",    about_path %></li>
              <li><%= link_to "Help",    help_path %></li>
              <li><%= link_to "News",   news_path %></li>
               <% if signed_in? %>
				      <li><%= link_to "Demo", demo_path %></li>
				      <li id="fat-menu" class="dropdown">
				        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
				          Account <b class="caret"></b>
				        </a>
				        <ul class="dropdown-menu">
				          <li><%= link_to "Profile", current_user %></li>
				          <li><%= link_to "Settings", '#' %></li>
				          <li class="divider"></li>
				          <li>
				            <%= link_to "Sign out", signout_path, method: "delete" %>
				          </li>
				        </ul>
				      </li>
				    <% else %>
				      <li><%= link_to "Sign in", signin_path %></li>
                  <li><%= link_to "Signup", signup_path %></li>
				    <% end %>
            </ul>
          </nav>
        </div>
      </div>
    </header>
