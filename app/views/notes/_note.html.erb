<li id = "<%= note.connection_id %>">
  <p><%= note.content %>  </p>
  <p><%= time_ago_in_words(note.created_at) %> ago. <%= link_to "delete", note, method:  :delete,
                                     confirm: "You sure?",
                                     title:   note.content, :remote => true, class: "deletenote" %></p>
</li>
